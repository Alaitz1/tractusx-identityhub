meta {
  name: Adds a service endpoint to a particular DID document.
  type: http
  seq: 6
}

post {
  url: {{IDENTITY_PATH}}/v1alpha/participants/{{PARTICIPANT_CONTEXT_ID}}/dids/{{DID}}/endpoints
  body: json
  auth: apikey
}

params:query {
  ~autoPublish: true
}

auth:apikey {
  key: X-Api-Key
  value: {{API_KEY}}
  placement: header
}

body:json {
  {
    "id": "did:web:super-user#service-1",
    "serviceEndpoint": "http://localhost:8182/api/identity",
    "type": "Service"
  }
}
